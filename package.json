{
  "name": "trafem",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "format": "prettier -w \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serverless-package": "serverless package",
    "redirect-config": "serverless print > .serverless/serverless.yml",
    "package": "npm-run-all -s \"serverless-package -- -s {1}\" \"redirect-config -- -s {1}\" --"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/validator": "^2.5.3",
    "axios": "^0.24.0",
    "class-validator": "^0.13.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "typeorm-aurora-data-api-driver": "^2.3.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.68.0",
    "@types/aws-lambda": "^8.10.87",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "aws-sdk": "^2.1046.0",
    "jest": "^27.4.4",
    "json-schema-to-ts": "^1.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "serverless": "^2.68.0",
    "serverless-esbuild": "^1.23.0",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
